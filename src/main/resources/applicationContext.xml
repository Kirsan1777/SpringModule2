<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/aop
    http://www.springframework.org/schema/aop/spring-aop-3.0.xsd ">

    <bean id="testBean"
          class="by.nikita.springcore.model.entity.TestBean">
        <constructor-arg value="Nikita"/>
    </bean>

    <bean id="clearTestBean"
          class="by.nikita.springcore.model.entity.TestBean">
    </bean>

    <bean id="knight"
          class="by.nikita.springcore.model.entity.Knight">
        <property name="phraseYes" value="Give me my sword!"/>
    </bean>

    <bean id="minstrel"
          class="by.nikita.springcore.model.entity.Minstrel">
    </bean>

    <bean id="giftCertificate"
          class="by.nikita.springcore.model.entity.GiftCertificate">

    </bean>

    <bean id="tag"
          class="by.nikita.springcore.model.entity.Tag">

    </bean>
</beans>